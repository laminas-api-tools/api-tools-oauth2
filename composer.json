{
    "name": "laminas-api-tools/api-tools-oauth2",
    "description": "Laminas module for implementing an OAuth2 server",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "api",
        "oauth2",
        "framework"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-oauth2/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-oauth2",
        "rss": "https://github.com/laminas-api-tools/api-tools-oauth2/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev",
            "dev-develop": "1.4-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "bshaffer/oauth2-server-php": "~1.7",
        "laminas-api-tools/api-tools-api-problem": "^1.2.1",
        "laminas-api-tools/api-tools-content-negotiation": "^1.2.1",
        "laminas/laminas-crypt": "~2.3",
        "laminas/laminas-http": "~2.3",
        "laminas/laminas-mvc": "^2.7.9",
        "laminas/laminas-servicemanager": "^2.7.6",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-authentication": "~2.3",
        "laminas/laminas-config": "~2.3",
        "laminas/laminas-db": "~2.3",
        "laminas/laminas-form": "~2.3",
        "laminas/laminas-i18n": "~2.3",
        "laminas/laminas-loader": "~2.3",
        "laminas/laminas-log": "~2.3",
        "laminas/laminas-modulemanager": "~2.3",
        "laminas/laminas-serializer": "~2.3",
        "laminas/laminas-test": "~2.3",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\OAuth2\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\OAuth2\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-oauth2": "self.version"
    }
}
